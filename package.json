{
  "name": "timetune",
  "private": true,
  "version": "0.5.1",
  "type": "module",
  "overrides": {
    "esbuild": "^0.25.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:party": "partykit dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:party\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "check": "npm run lint && npm audit --audit-level=moderate && npm run build",
    "deploy:party": "partykit deploy",
    "db:fetch-youtube": "tsx database/fetch-youtube-ids.ts",
    "db:generate-json": "tsx database/generate-songs-json.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "partysocket": "^1.1.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "knip": "^5.80.0",
    "partykit": "^0.0.115",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0"
  }
}
